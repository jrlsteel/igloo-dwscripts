create table ref_calculated_daily_customer_file
(
account_id bigint null,
elec_ssd datetime null,
gas_ssd datetime null,
elec_ed datetime null,
gas_ed datetime null,
acc_ssd  datetime null,
acc_ed  datetime null,
num_elec_mpns bigint null,
num_gas_mpns bigint null,
first_canc_elec_ssd  datetime null,
first_canc_elec_sed  datetime null,
first_canc_gas_ssd  datetime null,
first_canc_gas_sed  datetime null,
num_canc_elec_mpns bigint null,
num_canc_gas_mpns bigint null,
eac_industry double null,
aq_industry double null,
eac_igloo_ca  double null,
eac_igloo_ca_source  varchar(256) null,
aq_igloo_ca  double null,
aq_igloo_ca_source  varchar(256) null,
reg_pay_amount double null,
reg_pay_amount_ex_wu double null,
wu_amount double null,
wu_this_year tinyint(1) null,
reg_pay_date double null,
projected_savings double null,
balance double null,
elec_annual_spend double null,
gas_annual_spend double null,
elec_reg_status varchar(15) null,
elec_cancellation_reason  varchar(5000) null,
gas_reg_status varchar(15) null,
gas_cancellation_reason  varchar(5000) null,
elec_loss_type varchar(3) null,
gas_loss_type varchar(3) null,
supply_type varchar(5) null,
account_status varchar(15) null,
account_loss_type varchar(3) null,
gsp varchar(255) null,
ldz varchar(255) null,
wl0_date datetime null,
wl1_date datetime null,
home_move_in tinyint(1) null,
gas_et1  tinyint(1) null,
elec_et1  tinyint(1) null,
signup_channel  varchar(255) null,
signup_channel_secondary  varchar(255) null,
broker_name  varchar(255) null,
broker_urn  varchar(255) null,
signup_date  datetime null,
campaign  varchar(90) null,
quoted_eac  double null,
quoted_aq  double null,
quoted_total_spend  double null,
first_payment_success  tinyint(1) null,
num_payments  bigint null,
latest_dd_received_date  datetime null,
expected_payment_missing  tinyint(1) null,
outcode  varchar(255) null,
first_bill_date datetime null,
first_bill_type  varchar(15) null,
latest_bill_date  datetime null,
latest_bill_type  varchar(15) null,
num_bills  bigint null,
first_final_bill_date  datetime null,
first_final_refund_date  datetime null,
etlchange timestamp null
)
;

create index ref_calculated_daily_customer_file_account_id_index
	on ref_calculated_daily_customer_file (account_id);